{
  "parser": "babel-eslint",
  "env": {
    "es6": true,
    "browser": true,
    "node": true
  },
  "rules": {
    "semi": "off",
    "linebreak-style": "off",
    "require-jsdoc": "off",
    "object-curly-spacing": "off",
    "operator-linebreak": "off",
    "no-trailing-spaces": "off",
    "spaced-comment": "off",
    "indent": "off",
    "max-len": "off"
  },
  "extends": ["eslint:recommended", "google"]
}
